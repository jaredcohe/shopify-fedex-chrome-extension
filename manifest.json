{
  "name": "threes-fedex-chrome-extension",
  "version": "0.2",
  "description": "Takes data from shopify browser tab and puts it in FedEx shipping tab",
  "browser_action":
  {
    "default_icon": "icon.png",
    "default_title": "Shopify to FedEx"
  },
  "background":
  {
    "scripts": ["index.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging"
  ],
  "manifest_version": 2
}
